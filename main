#define _CRT_SECURE_NO_WARNINGS
#include <conio.h>
#include <stdio.h>

struct list
{
	list *next;
	char e;
};

FILE *in, *out;

list *input(list *head)
{
	in = fopen("input.txt", "r");
	head = new list;
	list *r = head;
	r->e = NULL;
	while (feof(in) == 0)

	{
		r->next = new list;
		r = r->next;
		fscanf_s(in, "%c", &r->e);
		r->next = NULL;
	}
	
	fclose(in);
	return head;
}

list *output(list *head)
{
	out = fopen("output.txt", "w");
	list *r = head->next;
	while (r)
	{
		fprintf_s(out, "%c", r->e);
		r = r->next;
	}
	fclose(out);
	return head;
}

list *reverse(list *head)
{
	if (!head) return NULL;

	list *curr = head, *next, *prev = NULL;

	while (curr)
	{
		next = curr->next;
		curr->next = prev;
		prev = curr;
		curr = next;
	}
	return prev;
}


